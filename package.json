{
  "name": "wikipedia-react-components",
  "description": "wikipedia style react components",
  "version": "1.6.7",
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jdlrobson/wikipedia-react-components.git"
  },
  "scripts": {
    "lint": "eslint . --ext .jsx --ext .js",
    "lint:fix": "eslint . --ext .jsx --ext .js  --fix",
    "compile": "webpack --output-path dist/",
    "build": "npm run compile && npm run storybook",
    "prepublish": "npm run compile",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "main": "dist/index.js",
  "style": "dist/styles.css",
  "devDependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "webpack": "^3.11.0",
    "@storybook/addon-actions": "^3.3.14",
    "@storybook/addon-links": "^3.3.14",
    "@storybook/addons": "^3.3.14",
    "@storybook/react": "^3.3.14",
    "babel-core": "^6.16.0",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.18.2",
    "extract-text-webpack-plugin": "^2.1.2"
  },
  "dependencies": {
    "css-loader": "^0.28.10",
    "eslint-config-wikimedia": "^0.5.0",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^1.1.11",
    "less-loader": "^4.0.6",
    "style-loader": "^0.20.2",
    "url-loader": "^0.6.2",
    "less": "^3.0.1",
    "wikimedia-ui-base": "github:wikimedia/WikimediaUI-Base"
  }
}
